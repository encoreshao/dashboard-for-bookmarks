{
  "manifest_version": 3,
  "name": "Bookmark Dashboard",
  "short_name": "Bookmarks",
  "version": "2.0.4",
  "description": "A beautiful dashboard for your Chrome bookmarks. Replace your new tab with an organized, searchable bookmark hub.",
  "author": "Encore Shao",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "background": {
    "service_worker": "js/service-worker.js"
  },
  "chrome_url_overrides": {
    "newtab": "src/index.html"
  },
  "options_page": "src/options/index.html",
  "permissions": [
    "bookmarks",
    "storage",
    "favicon"
  ]
}
